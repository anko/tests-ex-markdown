{
  "name": "tests-ex-markdown",
  "version": "4.0.0",
  "description": "Run markdown usage examples as unit tests",
  "main": "index.js",
  "scripts": {
    "prepare": "lsc --compile --bare --print index.ls | sed '1s:^:#!/usr/bin/env node\\n:' > index.js",
    "test": "lsc index.ls readme.markdown && lsc test.ls"
  },
  "bin": {
    "txm": "index.js"
  },
  "author": "Anko <an@cyan.io>",
  "engines": {
    "node": ">=0.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anko/tests-ex-markdown"
  },
  "bugs": "https://github.com/anko/tests-ex-markdown/issues",
  "license": "ISC",
  "files": [
    "index.js",
    "readme.markdown"
  ],
  "keywords": [
    "markdown",
    "test",
    "example",
    "readme",
    "comment",
    "tap"
  ],
  "dependencies": {
    "async": "3.2.0",
    "chalk": "^4.0.0",
    "concat-stream": "2.0.0",
    "diff-match-patch": "1.0.4",
    "parse5-sax-parser": "5.1.1",
    "prelude-ls": "1.1.2",
    "remark-parse": "^8.0.0",
    "unified": "^9.0.0",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "livescript": "1.6.0",
    "tape": "4.13.2"
  }
}
