{
  "name": "tests-ex-markdown",
  "version": "1.3.0",
  "description": "Run markdown usage examples as unit tests",
  "main": "index.js",
  "scripts": {
    "prepublish": "lsc --compile --bare --print index.ls | sed '1s:^:#!/usr/bin/env node\\n:' > index.js",
    "test": "lsc index.ls readme.markdown && lsc test.ls"
  },
  "bin": {
    "txm": "index.js"
  },
  "author": "Anko <an@cyan.io>",
  "engines": {
    "node": ">=0.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anko/tests-ex-markdown"
  },
  "bugs": "https://github.com/anko/tests-ex-markdown/issues",
  "license": "ISC",
  "files": [
    "index.js",
    "readme.markdown"
  ],
  "keywords": [
    "markdown",
    "test",
    "example",
    "readme",
    "comment",
    "tap"
  ],
  "dependencies": {
    "async": "^1.4.2",
    "concat-stream": "^1.4.8",
    "mdast": "^1.1.0",
    "minimist": "^1.2.0",
    "parse5": "^1.5.0",
    "prelude-ls": "^1.1.1",
    "tape": "^4.0.0",
    "tapsoneer": "^0.1.0"
  },
  "devDependencies": {
    "LiveScript": "^1.3.1"
  }
}
